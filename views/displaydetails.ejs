<%- include("partials/header.ejs") %>



<table class="table table-striped table-hover">
    <tr>
      <th>Item ID</th>
      <th>Item Name</th>
      <th>Category</th>
      <th>Brand</th>
      <th>UOM</th>
      <th>Spec</th>
    </tr>
    <% if (locals.entries) { %>
      <% for (let x of entries){%>
    <tr>
      <td><%=x.id%></td>
      <td><%=x.name%></td>
      <td><%=x.category%></td>
      <td><%=x.brand%></td>
      <td><%=x.uom%></td>
      <td><%=x.spec%></td>
    </tr>
    <% };} else {%>
      <p>No record found!</p>
      <% } %>
</table>
  
  <% if (locals.message) { %>
  <p><%= message %></p>  
  <% } %>
  
<%- include("partials/footer.ejs") %>